{
    "manifest_version": 3,
    "name": "Binghamton Athletics",
    "version": "1.2",
    "description": "Binghamton Athletics custom home page",
    "permissions": ["tabs", "history"],
    "chrome_url_overrides": {
      "newtab": "index.html"
    },
    "background": {
      "service_worker": "assets/js/background.js"
    },
    "action": {
      "default_icon": {
        "16": "assets/icons/bicon16.png",
        "48": "assets/icons/bicon48.png",
        "128": "assets/icons/bicon128.png"
      }
    },
    "icons": {
      "16": "assets/icons/bicon16.png",
      "48": "assets/icons/bicon48.png",
      "128": "assets/icons/bicon128.png"
    }
  }